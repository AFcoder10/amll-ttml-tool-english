{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "AMLL TTML Tool",
  "mainBinaryName": "AMLL TTML Tool",
  "version": "0.0.0",
  "identifier": "net.stevexmh.amll-ttml-tool",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Apple Music-like lyric 插件 TTML 歌词工具",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}